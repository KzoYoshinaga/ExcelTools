Attribute VB_Name = "TupleTest"
' Tuple **********************************************

' 新しいインスタンスが生成されること
Function createNewInstance()
    ' SetTestName
    testName = "createNewInstance()"

    ' Arrange
    Dim values As New collection
    values.add newElement("a")
     
    ' Do
    Dim t As New Tuple
    t.setValues = values
    
    ' Verification
    result = equals(t.toString, "Tuple(a)")
    verify result, testName
End Function

